  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "electron": "^26.2.2",
    "electron-builder": "^24.6.4",
    "typescript": "^5.2.2"
  },
  "build": {
    "appId": "scottish.glen",
    "productName": "Scottish Glen",
    "files": [
      "dist/**/*",
      "html/**/*"
    ],
    "directories": {
      "output": "release"
