{
  "name": "project-liamtownsley2",
  "version": "0.0.1",
  "description": "This is my Portfolio project for CMP307",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmp307/project-LiamTownsley2.git"
  },
  "scripts": {
